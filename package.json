{
  "name": "BudgetControl",
  "homepage": "https://www.budgetcontrol.cloud",
  "version": "1.1.0",
  "description": "Budget control free app for budget monitoring",
  "repository": "https://github.com/BudgetControl/",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && gulp licenses",
    "build-dev": "vue-cli-service build --mode dev --report-json && gulp licenses",
    "build-mobile": "vue-cli-service build --dest ../Cordova-APP/www",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build -i src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve",
    "start": "npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@popperjs/core": "2.9.1",
    "@vue/compiler-sfc": "^3.3.4",
    "@vuepic/vue-datepicker": "^7.0.0",
    "axios": "^1.3.6",
    "chart.js": "2.9.4",
    "core-js": "3.9.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "vue": "^3.3.4",
    "vue-accessible-color-picker": "^4.1.4",
    "vue-full-loading": "^1.2.1",
    "vue-i18n": "^9.9.1",
    "vue-router": "4.0.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@tailwindcss/forms": "^0.4.0-alpha.2",
    "@vue/cli-plugin-babel": "5.0.0-alpha.7",
    "@vue/cli-plugin-eslint": "5.0.0-alpha.7",
    "@vue/cli-service": "5.0.0-alpha.7",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.3.3",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.5",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
